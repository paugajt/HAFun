<p id="notice"><%= notice %></p>

<p>
  <strong>Date:</strong>
  <%= @match.date %>
</p>

<p>
  <strong>Time:</strong>
  <%= @match.time %>
</p>

<ul>
  <% @match.players.each do |player| %>
    <li><%= player.name %></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_match_path(@match) %> |
<%= link_to 'Back', matches_path %>
